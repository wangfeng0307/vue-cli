{
  "presets": [
    ["env", {
      "modules": false,//tree shaking
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-0"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime"],
  "env": {
    "test": {
      "presets": ["env", "stage-0"],
      // 测试单元覆盖率
      "plugins": ["instanbul"]
    }
  }
}
